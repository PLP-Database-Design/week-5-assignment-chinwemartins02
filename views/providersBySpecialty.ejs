<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Providers by Specialty</title>
  <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
      padding: 8px;
    }
    th, td {
      text-align: left;
    }
    h2 {
      margin-top: 20px;
    }

    th{
        background: magenta;
    }
    td{
        background: aliceblue;
    }
  </style>
</head>
<body>
  <h1>Providers by Specialty</h1>
  <% for (const specialty in providersBySpecialty) { %>
    <h2><%= specialty %></h2>
    <table>
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
        </tr>
      </thead>
      <tbody>
        <% providersBySpecialty[specialty].forEach(provider => { %>
          <tr>
            <td><%= provider.first_name %></td>
            <td><%= provider.last_name %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  <% } %>
</body>
</html>
